class Solution {
    public int lengthOfLongestSubstring(String s) {
        int max=0;
        int j=0;
        HashSet<Character> h=new HashSet<>();
        for(int i=0;i<s.length();i++){
            while(h.contains(s.charAt(i))){
                h.remove(s.charAt(j));
                j++;
            }
                h.add(s.charAt(i));
            max=Math.max(max,i-j+1);
        }
        return max;
    }
}
